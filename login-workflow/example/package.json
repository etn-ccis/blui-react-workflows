    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "IE 11",
            "not IE 9-10"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11",
            "not ie 9-10"
        ]
